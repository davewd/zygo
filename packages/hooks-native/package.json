{
  "dependencies": {
    "@zygo/constants": "workspace:*",
    "@zygo/eslint-config": "workspace:*",
    "@zygo/typescript-config": "workspace:*",
    "expo-image": "latest",
    "react": "latest",
    "react-native": "latest"
  },
  "description": "Zygo React hooks library",
  "devDependencies": {
    "@types/react": "latest",
    "@types/react-native": "latest",
    "jest": "latest",
    "tailwindcss": "^4.1.8",
    "ts-jest": "latest",
    "typescript": "latest"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "name": "@zygo/hooks-native",
  "private": true,
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declaration --declarationMap",
    "clean": "rimraf dist tsconfig.tsbuildinfo .turbo node_modules",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.1"
}
