{
  "dependencies": {
    "@zygo/eslint-config": "workspace:*",
    "@zygo/typescript-config": "workspace:*",
    "react": "19.1.0"
  },
  "description": "Zygo monorepo",
  "devDependencies": {
    "clsx": "2.1.1",
    "jest": "latest",
    "tailwindcss": "^4.1.8",
    "ts-jest": "latest",
    "typescript": "latest"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "name": "@zygo/types",
  "private": true,
  "scripts": {
    "build": "tsup && tsc --declaration --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist tsconfig[ 0-9]+.tsbuildinfo .turbo node_modules",
    "dev": "tsup --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.1"
}
