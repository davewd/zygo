{
  "$schema": "https://turbo.build/schema.v1.json",
  "tasks": {
    "lint": {
      "outputs": []
    },
    "clean:build": {
      "cache": false,
      "persistent": true,
      "outputs": []
    },
    "clean": {
      "cache": false,
      "dependsOn": [
        "clean:build"
      ],
      "persistent": true,
      "outputs": []
    },
    "prebuild": {
      "cache": false,
      "persistent": true,
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "**/*.{ts,tsx,js,jsx}"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputLogs": "new-only",
      "outputs": [
        "build/**",
        "node_modules/.cache/metro/**"
      ]
    },
    "@zygo/types#build": {
      "dependsOn": [
        "@zygo/typescript-config#build",
        "@zygo/eslint-config#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@zygo/constants#build": {
      "dependsOn": [
        "@zygo/typescript-config#build",
        "@zygo/eslint-config#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@zygo/libs#build": {
      "dependsOn": [
        "@zygo/typescript-config#build",
        "@zygo/eslint-config#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@zygo/hooks#build": {
      "dependsOn": [
        "@zygo/constants#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@zygo/ui#build": {
      "dependsOn": [
        "@zygo/typescript-config#build",
        "@zygo/eslint-config#build",
        "@zygo/libs#build",
        "@zygo/types#build",
        "@zygo/hooks#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@zygo/ui-native#build": {
      "dependsOn": [
        "@zygo/typescript-config#build",
        "@zygo/eslint-config#build",
        "@zygo/libs#build",
        "@zygo/types#build",
        "@zygo/hooks#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@zygo/web#build": {
      "dependsOn": [
        "@zygo/ui#build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "@zygo/mobile#build": {
      "dependsOn": [
        "@zygo/ui#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test:watch": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    }
  },
  "globalDependencies": [
    "tsconfig.json"
  ]
}